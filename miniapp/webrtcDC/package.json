{
  "name": "webrtcDC",
  "version": "0.0.0",
  "description": "基于webrtc扩展的库",
  "source": "src/index.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "webpack",
    "copy:ts": "cp src/index.js dist/index.ts",
    "build:d.ts": "tsc --declaration dist/index.ts",
    "remove:ts": "rm -f dist/index.ts",
    "prepublish": "npm run copy:ts && npm run build:d.ts && npm run build"
  },
  "keywords": [
    "s-bridge",
    "dsBridge",
    "ios",
    "android"
  ],
  "files": [
    "package.json",
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@types/estree": "^0.0.47",
    "dsbridge": "^3.1.4"
  },
  "devDependencies": {
    "typescript": "5.5.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "microbundle": "^0.12.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
